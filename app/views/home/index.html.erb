<header>
<div class="inline">
<%= image_tag("portrait.png", :alt => "portrait", :class => "portrait_dimension") %>
</div>
<div class="container text_align">
<h1>Jackson Sandland</h1>
<h2>Full Stack Web Developer and Music Producer</h2>
<%= link_to "Github  /", "https://github.com/jackson-sandland", { :class=>"text_decoration" } %>
<%= link_to "Linked-In  /", "https://linkedin.com/in/jacksonsandland/", { :class=>"text_decoration" } %>
<%= link_to "Resume", "/assets/images/Resume.pdf", { :class=>"text_decoration" } %>
</div>
</header>
<!doctype html>
<html>
<head>
  <meta name="viewport" content="width=device-width">

</head>
<body>
  <div class="container">
    <div id="slides">
    <%= image_tag("example-slide-1.jpg") %>
    <%= image_tag("example-slide-2.jpg") %>
    <%= image_tag("example-slide-3.jpg") %>
    <%= image_tag("example-slide-4.jpg") %>
    </div>
  </div>

  
  <script>
    $(function() {
      $('#slides').slidesjs({
        width: 940,
        height: 528,
        play: {
          active: true,
          auto: true,
          interval: 4000,
          swap: true
        }
      });
    });
  </script>
</body>
</html>
